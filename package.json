{
  "name": "react-target-scroller",
  "version": "0.1.12",
  "description": "React component that scrolls smoothly to the target element passed in via props.",
  "license": "MIT",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "repository": "ryanhefner/react-target-scroller",
  "bugs": {
    "url": "https://github.com/ryanhefner/react-target-scroller/issues"
  },
  "homepage": "https://github.com/ryanhefner/react-target-scroller#readme",
  "keywords": [
    "scroll",
    "target",
    "element",
    "smooth-scroll",
    "react-component",
    "react",
    "component",
    "tween",
    "tweenkle"
  ],
  "files": [
    "es",
    "index.js",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -fr es && rm -fr umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__,**/*.test.js --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "jest --findRelatedTests src/index.js"
  },
  "peerDependencies": {
    "react": ">=16.3"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.1",
    "tweenkle": "^0.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "coveralls": "^3.0.6",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "greenkeeper-lockfile": "^1.15.0",
    "gzip-size": "^5.0.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.0",
    "jsdom": "^15.1.1",
    "pretty-bytes": "^5.3.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.0",
    "sinon": "^8.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "setupFiles": [
      "<rootDir>/tools/jest-setup.js"
    ],
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ],
    "testURL": "http://localhost"
  }
}
